   "source": [
    "import re\n",
    "#re.sub('<.*?>', '', string)\n",
    "\n",
    "conversations_3 = {}\n",
    "for i in range(len(data)):\n",
    "    record_id = i\n",
    "    record = data[i]\n",
    "    app_id = record[\"applicant_id\"]\n",
    "    app_id = str(app_id)\n",
    "    applicant_auth_ident = \"applicant_\" + app_id\n",
    "    curr_discussion = record[\"discussion\"]\n",
    "    \n",
    "    current_convo = []\n",
    "    for speaker_line in curr_discussion:\n",
    "        if speaker_line[\"author_ident\"] == applicant_auth_ident:\n",
    "            applicant_response = speaker_line[\"content\"]\n",
    "            str_response = str(applicant_response)\n",
    "            response_no_action_brackets = re.sub('<.*?>', '',str_response)\n",
    "            current_convo.append(response_no_action_brackets)\n",
    "    convo_as_only_str = \" \".join(current_convo)\n",
    "    conversations_3[i] = convo_as_only_str"
   ]
