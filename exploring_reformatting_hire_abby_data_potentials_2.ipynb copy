  {
   "cell_type": "code",
   "execution_count": 164,
   "metadata": {},
   "outputs": [],
   "source": [
    "import string\n",
    "from nltk.tokenize import sent_tokenize\n",
    "from nltk.corpus import stopwords\n",
    "\n",
    "def simple_tokenize_clean(s):\n",
    "    stoplist = stopwords.words('english')\n",
    "    stoplist+=['?','!','.',',',':',';',\"this\"]\n",
    "    punct_to_remove = re.compile('[%s]' % re.escape(string.punctuation))\n",
    "    no_punct_s = punct_to_remove.sub('', s)\n",
    "    token_text_clean = [word for word in no_punct_s.lower().split() if word not in stoplist]\n",
    "    \n",
    "    return(token_text_clean)\n",
    "    \n",
    "\n",
    "\n",
    "def clean_rawtext_in_dict(reorganized_dict_of_potentials):\n",
    "    for record in reorganized_dict_of_potentials:\n",
    "        #print( record)\n",
    "        curr_rec = reorganized_dict_of_potentials[record]\n",
    "        raw_replies = curr_rec[\"applicant_replies\"]#ID, Placement, and Score, next 3 lines\n",
    "        clean_replies = simple_tokenize_clean(raw_replies)\n",
    "        curr_rec[\"applicant_replies_clean\"] = clean_replies\n",
    "        #print (curr_rec)\n",
    "    return(reorganized_dict_of_potentials)\n",
    "        \n",
    "\n",
    "# apply text_cleaning function to dictionary structure\n",
    "# return dictionary with addition of cleaned text\n",
    "# maybe dict should have: \n",
    "#      \"applicant_replies_raw\" \n",
    "#     \"applicant_replies_cleaned\"\n",
    "#      \"average_doc_2_vec_score\"\n",
    "\n"
   ]
  },
