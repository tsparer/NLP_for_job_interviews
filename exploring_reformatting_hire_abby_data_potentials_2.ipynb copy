  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Function to take a document as a list of words and return the document vector\n",
    "# from class code\n",
    "\n",
    "\n",
    "\n",
    "# \n",
    "\n",
    "\n",
    "\n",
    "def get_doc_vec(words, model):\n",
    "    \"\"\"For each word in a document (where a document is a list of words as strings), \n",
    "    this takes the word, finds its location in word2vec space, and, once all words\n",
    "    are mapped word vectors in word2vec space, averages across all of those to find the\n",
    "    average document vector\"\"\"\n",
    "    \n",
    "    good_words = []\n",
    "    for word in words:\n",
    "        # Words not in the original model will fail\n",
    "        try:\n",
    "            if model.wv[word] is not None:\n",
    "                good_words.append(word)\n",
    "        except:\n",
    "            continue\n",
    "    # If no words are in the original model\n",
    "    if len(good_words) == 0:\n",
    "        return None\n",
    "    # Return the mean of the vectors for all the good words\n",
    "    return model.wv[good_words].mean(axis=0)"
   ]
  },
