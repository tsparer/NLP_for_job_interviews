   "source": [
    "# Now, turn each candidate's overall reply into a doc2vec score (average)\n",
    "import os\n",
    "import gensim\n",
    "\n",
    "# Setup nltk corpora path and Google Word2Vec location\n",
    "\n",
    "google_vec_file = '/Users/tim/Downloads/GoogleNews-vectors-negative300.bin'\n",
    "model = gensim.models.KeyedVectors.load_word2vec_format(google_vec_file, binary=True)\n",
    "\n",
    "def get_doc_vec(words, model):\n",
    "    \"\"\"For each word in a document (where a document is a list of words as strings), \n",
    "    this takes the word, finds its location in word2vec space, and, once all words\n",
    "    are mapped word vectors in word2vec space, averages across all of those to find the\n",
    "    average document vector\"\"\"\n",
    "    \n",
    "    good_words = []\n",
    "    for word in words:\n",
    "        # Words not in the original model will fail\n",
    "        try:\n",
    "            if model.wv[word] is not None:\n",
    "                good_words.append(word)\n",
    "        except:\n",
    "            continue\n",
    "    # If no words are in the original model\n",
    "    if len(good_words) == 0:\n",
    "        return None\n",
    "    # Return the mean of the vectors for all the good words\n",
    "    return model.wv[good_words].mean(axis=0)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def doc2vec_on_cleaned_potentials(cleaned_dict_of_potentials, model):\n",
    "    for record in cleaned_dict_of_potentials:\n",
    "        curr_rec = cleaned_dict_of_potentials[record]\n",
    "        clean_replies = record[\"applicant_replies_clean\"]        #ID, Placement, and Score, next 3 lines\n",
    "        doc_vec = get_doc_vec(clean_replies, model)\n",
    "        record[\"average_doc_2_vec_score\"] = doc_vec\n",
    "                              \n",
    "\n",
    "def clean_rawtext_in_dict(reorganized_dict_of_potentials):\n",
    "    for record in reorganized_dict_of_potentials:\n",
    "        #print( record)\n",
    "        curr_rec = reorganized_dict_of_potentials[record]\n",
    "        raw_replies = curr_rec[\"applicant_replies\"]#ID, Placement, and Score, next 3 lines\n",
    "        clean_replies = simple_tokenize_clean(raw_replies)\n",
    "        curr_rec[\"applicant_replies_clean\"] = clean_replies\n",
    "        #print (curr_rec)\n",
    "    return(reorganized_dict_of_potentials)\n",
    "\n",
    "\n",
    "#convos_dict_with_vectors:\n",
